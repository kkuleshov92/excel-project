{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "indent": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "no-trailing-spaces": "off"
    },
    "extends": [
        "eslint:recommended", 
        "google"
    ]
}